SN1COD = sn1_16.cod sn1_2.cod sn1_32.cod sn1_4.cod sn1_64.cod	\
sn1_8.cod


ST1 = st1_2_4.c st1_2_8.c st1_2_16.c st1_2_32.c st1_2_64.c		\
st1_2_128.c st1_2_256.c st1_2_512.c st1_2_1024.c st1_4_4.c st1_4_8.c	\
